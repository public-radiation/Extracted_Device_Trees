        linux,phandle = <0x11>;
        phandle = <0x11>;
    };
 
    io-domains {
        compatible = "rockchip,rk3288-io-voltage-domain";
        rockchip,grf = <0x9>;
        sdcard-supply = <0x12>;
    };
 
    board-leds {
        compatible = "gpio-leds";
        status = "okay";
 
        work {
            label = "System";
            linux,default-trigger = "timer";
            default-state = "on";
            gpios = <0x6c 0x2 0x0>;
        };
    };
 
    gpio-fan {
        status = "okay";
        compatible = "gpio-fan";
        gpios = <0x76 0x9 0x0>;
        gpio-fan,speed-map = <0x0 0x0 0x3e8 0x1>;
        #cooling-cells = <0x2>;
        linux,phandle = <0x74>;
        phandle = <0x74>;
    };
};
